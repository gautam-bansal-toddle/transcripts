{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "16"
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    ["@babel/preset-typescript"]
  ],
  "plugins": [
    [
      "babel-plugin-react-compiler",
      {
        "target": "17",
        "compilationMode": "all"
      }
    ]
  ]
}
